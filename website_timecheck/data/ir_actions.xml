<?xml version="1.0" encoding="utf-8"?>
<openerp>
<data>

    <record id="publish_product_action" model="ir.actions.server">
        <field name="name">Publish Product(s)</field>
        <field name="model_id" ref="product.model_product_template"/>
        <field name="state">code</field>
        <field name="code">self.browse(cr,uid,context.get('active_ids', [])).write({'website_published': True})</field>
    </record>

    <record id="publish_product" model="ir.values">
        <field eval="'client_action_multi'" name="key2"/>
        <field eval="'product.template'" name="model"/>
        <field name="name">publish.product</field>
        <field eval="'ir.actions.server,%d'%publish_product_action" name="value"/>
    </record>

    <record id="unpublish_product_action" model="ir.actions.server">
