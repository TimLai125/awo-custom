<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data>

        <record id="product_product_tree_view_z1" model="ir.ui.view">
            <field name="name">product.product.tree.view.z1</field>
            <field name="model">product.product</field>
            <field name="inherit_id" ref="product_listprice_list_view.product_product_tree_view_z1"/>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='qty_local_stock']" position="before">
                    <field name="qty_local_own_stock" widget="integer" string="HK Own Stock"/>
                    <field name="qty_local_supplier_stock" widget="integer" string="HK Supplier Stock"/>
                </xpath>
                <xpath expr="//field[@name='qty_local_stock']" position="attributes">
                    <attribute name="invisible">1</attribute>
                </xpath>
            </field>
        </record>

    </data>
</openerp>

